# Ретейл-компании с полными данными: год основания, страна, организационно-правовая форма, количество сотрудников
SELECT DISTINCT ?company ?companyLabel ?inceptionYear ?countryLabel ?legalFormLabel ?employees WHERE {
  ?company wdt:P31/wdt:P279* wd:Q126793.  # "instance of" или "subclass of" = "retail company"

  # Год основания (обязательно)
  ?company wdt:P571 ?inceptionDate. BIND(YEAR(?inceptionDate) AS ?inceptionYear)

  # Страна происхождения (обязательно)
  ?company wdt:P17 ?country.

  # Организационно-правовая форма (обязательно)
  ?company wdt:P1454 ?legalForm.

  # Количество сотрудников (обязательно)
  ?company wdt:P1128 ?employees.

  # Метки для удобства чтения
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],ru,en". }
}

